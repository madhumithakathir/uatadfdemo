															"container_sink": {
																"value": "@item().container_sink",
																"type": "Expression"
															},
															"directory_sink": "@concat(split(item().File_name_sink,'.')[0],'/Date_Part=',formatDateTime(utcnow(),'yyyy-MM-dd'))"
