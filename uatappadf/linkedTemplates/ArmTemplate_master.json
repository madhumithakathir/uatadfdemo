{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "uatappadf"
		},
		"AzureDataLakeStorage_JSON_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://uatadlssource.dfs.core.windows.net"
		},
		"AzureDataLakeStorage_JSON_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "492f0511-f2cc-4b1a-8d47-9b4b56ed6bec"
		},
		"AzureDataLakeStorage_JSON_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "4d8e5375-0cf7-4f92-8799-d7d8472708a4"
		},
		"AzureDataLakeStorage_Sink_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://uatadlsraw.dfs.core.windows.net"
		},
		"AzureDataLakeStorage_Sink_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "492f0511-f2cc-4b1a-8d47-9b4b56ed6bec"
		},
		"AzureDataLakeStorage_Sink_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "4d8e5375-0cf7-4f92-8799-d7d8472708a4"
		},
		"AzureDataLakeStorage_Source_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://uatadlssource.dfs.core.windows.net"
		},
		"AzureDataLakeStorage_Source_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "492f0511-f2cc-4b1a-8d47-9b4b56ed6bec"
		},
		"AzureDataLakeStorage_Source_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "4d8e5375-0cf7-4f92-8799-d7d8472708a4"
		},
		"AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://uatappkeyvault.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "uatappadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"AzureDataLakeStorage_JSON_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_JSON_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage_JSON_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStorage_JSON_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStorage_JSON_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataLakeStorage_JSON_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureDataLakeStorage_Sink_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_Sink_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage_Sink_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStorage_Sink_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStorage_Sink_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataLakeStorage_Sink_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureDataLakeStorage_Source_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_Source_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage_Source_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStorage_Source_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStorage_Source_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataLakeStorage_Source_properties_typeProperties_servicePrincipalId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}